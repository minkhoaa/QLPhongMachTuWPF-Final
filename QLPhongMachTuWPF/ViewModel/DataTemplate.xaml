<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:view="clr-namespace:QLPhongMachTuWPF.View"
                    xmlns:vm="clr-namespace:QLPhongMachTuWPF.ViewModel">
    
    <DataTemplate DataType="{x:Type vm:HomepageVM}">
        <view:Homepage/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PatientsVM}">
        <view:Patients />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:StaffsVM}">
        <view:Staffs/>
    </DataTemplate>


    <DataTemplate DataType="{x:Type vm:ManageAppointmentsVM}">
        <view:ManageAppointments/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:DiagnosisVM}">
        <view:Diagnosis/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ListsVM}">
        <view:Lists/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:InvoiceVM}">
        <view:Invoice/>
    </DataTemplate>

</ResourceDictionary>